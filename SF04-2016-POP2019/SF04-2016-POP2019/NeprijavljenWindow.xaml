<Window x:Class="SF04_2016_POP2019.NeprijavljenWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SF04_2016_POP2019"
        mc:Ignorable="d"
        Title="NeprijavljenWindow" Height="450" Width="800">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="Black" Offset="1"/>
            <GradientStop Color="#FFA8FFAC"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <DataGrid AutoGeneratingColumn="dgFaculty_AutoGeneratingColumn" x:Name="dgFakulteti" HorizontalAlignment="Left" Height="276" Margin="28,50,0,0" VerticalAlignment="Top" Width="732" Grid.Column="1" ColumnWidth="*" />
        <Button Click="btnProfesoriAsistenti_Click" Content="Profesori i asistenti" HorizontalAlignment="Left" Height="53" Margin="77,356,0,0" VerticalAlignment="Top" Width="278" FontSize="18" FontWeight="Bold" Background="White"/>
        <Button Click="btnPredavanjaVezbe_Click" Content="Predavanja i vezbe" HorizontalAlignment="Left" Height="53" Margin="431,356,0,0" VerticalAlignment="Top" Width="278" FontSize="18" FontWeight="Bold" Background="White"/>
    </Grid>
</Window>
